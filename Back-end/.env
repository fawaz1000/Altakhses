# --------------------------------------------
# رابط اتصال MongoDB (يمكنك تعديله لـ Atlas أو أي عنوان آخر)
MONGODB_URI=mongodb://127.0.0.1:27017/clinicDB

# --------------------------------------------
# السرّ المستخدم لتوقيع وفحص JWT
# اجعله مُعقّداً وفريداً (لا تشارك هذا المفتاح)
JWT_SECRET=your_super_secret_jwt_key_here

# --------------------------------------------
# المنفذ الذي سيعمل عليه السيرفر (اختياري، الافتراضي 5050)
PORT=5050

# --------------------------------------------
# أصل طلبات CORS المسموح بها من الواجهة (React)
CORS_ORIGIN=http://localhost:3000
